{
  "name": "Sendpushlib",
  "version": "0.1.0",
  "summary": "Library for use with Sendpush",
  "description": "This library needs to be included in your iOS app to allow sendpush functionality.",
  "homepage": "https://github.com/sendpush/sendpush-ios-lib",
  "license": "MIT",
  "authors": {
    "Bob Axford": "bob@figjamit.com.au"
  },
  "source": {
    "git": "https://github.com/sendpush/sendpush-ios-lib",
    "tag": "0.1.0"
  },
  "platforms": {
    "ios": "8.0"
  },
  "requires_arc": true,
  "source_files": "Pod/Classes/**/*",
  "resource_bundles": {
    "Sendpushlib": [
      "Pod/Assets/*.png"
    ]
  },
  "dependencies": {
    "JSONWebToken": [
      "1.4.0"
    ]
  }
}
